@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class AddressBook
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class UniqueClassTagList
Class Person
Class Address
Class Email
Class Name
Class Phone
Class StudentId
Class ClassTag
Class AttendanceList
Class Attendance
Class AttendanceHistoryEntry
Class AttendanceHistorySummary
Class Date
Class FeeTracker
Class FeeState
Class FeeHistoryEntry
Class FeeHistorySummary
Class Month
Class PerformanceList
Class PerformanceNote
Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyAddressBook
ModelManager -left-> "1" AddressBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniquePersonList
AddressBook *--> "1" UniqueClassTagList
AddressBook *--> "1" FeeTracker

ModelManager --> FeeTracker

UniquePersonList --> "~* all" Person
UniqueClassTagList --> "~* all" ClassTag
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> StudentId
Person *--> "*" ClassTag
Person *--> AttendanceList
Person --> "1" Month : enrolledMonth
Person *--> PerformanceList

AttendanceList --> "*" Attendance

Attendance --> Date
Attendance --> ClassTag

AttendanceHistoryEntry --> Date
AttendanceHistoryEntry --> ClassTag

AttendanceHistorySummary --> StudentId
AttendanceHistorySummary --> Date

PerformanceList --> "*" PerformanceNote

PerformanceNote --> Date
PerformanceNote --> ClassTag

FeeHistoryEntry --> Month
FeeHistoryEntry --> FeeState

FeeHistorySummary --> StudentId
FeeHistorySummary --> Month

Person -[hidden]up--> I
UniquePersonList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Person
ModelManager --> "~* displayed" PerformanceNote
ModelManager --> "~* history" FeeHistoryEntry
ModelManager --> "~* history" AttendanceHistoryEntry
ModelManager --> FeeHistorySummary
ModelManager --> AttendanceHistorySummary

AttendanceHistorySummary -[hidden]right-> FeeHistoryEntry
FeeHistoryEntry -[hidden]right-> FeeHistorySummary
@enduml